// Coloque su configuración en este archivo para sobrescribir la configuración predeterminada y de usuario.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/bin": true,
        "**/obj": true
    },
    "json.schemas": [
        {
            "fileMatch": [
                "xunit*"
            ],
            "url": "http://xunit.github.io/schema/v2.1-rc1/xunit.runner.schema.json"
        }
    ]
}